<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.ctbu.lc.dao.TestDao">
    <insert id="insertData" parameterType="com.ctbu.lc.entity.TestEntity" useGeneratedKeys="true" keyProperty="id">
        insert into test (name)
        values (#{testEntity.name});
    </insert>

    <update id="updateData">
        update test
        set name = #{name}
        where id = #{id}
    </update>
</mapper>